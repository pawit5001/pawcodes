local Section = Tab:AddSection({
	Name = "Auto Buy"
})

Tab:AddToggle({
    Name = "Auto Buy Auras",
    Default = state,
    Callback = function(state)
_G.autobuyaura = state
while _G.autobuyaura do wait()
local auraNames = {}
for _, aura in ipairs(game:GetService("ReplicatedStorage").Auras:GetChildren()) do
    table.insert(auraNames, aura.Name)
end

local args = {
    [1] = "Unlock",
    [2] = auraNames[2]
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Aura"):FireServer(unpack(args))
wait()
end
    end
})

