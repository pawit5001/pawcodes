local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/pawit5001/pawcodes/main/uithankstoOrionLib')))()
local Window = OrionLib:MakeWindow({Name = "MBLover", HidePremium = false, SaveConfig = true, ConfigFolder = "MB"})
local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = Tab:AddSection({
	Name = "Auto Farm"
})
Tab:AddToggle({
    Name = "Auto Muscle",
    Default = state,
    Callback = function(Value)
        _G.automuscle = Value
        while _G.automuscle do
            wait()
            local tool = game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Tool")
            if tool then
                local args = {
                    [1] = tool.Name
                }
                game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Weight"):FireServer(unpack(args))
                wait(.1)
            end
            local args = {
                [1] = "NumberOne",
                [2] = Value 
            }
            game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("SkillToggle"):FireServer(unpack(args))
        end
    end    
})

Tab:AddToggle({
    Name = "Auto Rebirth",
    Default = state,
    Callback = function(state)
_G.autorebirth = state
while _G.autorebirth do wait()
game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Rebirth"):FireServer()
wait()
end
    end
})

Tab:AddToggle({
    Name = "Auto Easter -OP Might make you lag-",
    Default = false,
    Callback = function(Value)
        _G.autoeaster = Value
        while _G.autoeaster do
            for i,v in pairs(game:GetService("Workspace").NPCS:GetDescendants()) do
                if v:IsA("Humanoid") and v.Parent:FindFirstChild("HumanoidRootPart") then
                    local distance = (v.Parent.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                    if distance <= math.huge then 
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Parent.HumanoidRootPart.CFrame + Vector3.new(0, 3, 2)
                    end
                end
            end

            game:GetService("ReplicatedStorage").Skills:WaitForChild("GroundPunchFolder"):WaitForChild("RemoteEvent"):FireServer()
            game:GetService("ReplicatedStorage").Skills:WaitForChild("LightningStrikeFolder"):WaitForChild("RemoteEvent"):FireServer()
            game:GetService("ReplicatedStorage").Skills:WaitForChild("KamehamehaFolder"):WaitForChild("RemoteEvent"):FireServer()
            game:GetService("ReplicatedStorage").Skills:WaitForChild("SmashFolder"):WaitForChild("RemoteEvent"):FireServer()
            game:GetService("ReplicatedStorage").Skills:WaitForChild("MultiplePunchesFolder"):WaitForChild("RemoteEvent"):FireServer()

            local args = {
                [1] = "Left"
            }
            game:GetService("ReplicatedStorage").Skills.CombatFolder.RemoteEvent:FireServer(unpack(args))

            local args = {
                [1] = "Right"
            }
            game:GetService("ReplicatedStorage").Skills.CombatFolder.RemoteEvent:FireServer(unpack(args))

            local args = {
                [1] = "NumberTwo",
                [2] = true
            }
            game:GetService("ReplicatedStorage").Events.SkillToggle:FireServer(unpack(args))
            wait(.1)
        end 
        print(Value)
    end    
})

