local Tab = Window:MakeTab({
	Name = "Others",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Auto Gifts"
})
Tab:AddToggle({
    Name = "Auto Redeem Free Gifts",
    Default = state,
    Callback = function(state)
_G.autogifts = state
while _G.autogifts do wait()
local args = {
    [1] = "Potion1",
    [2] = "Muscle1",
    [3] = "Gem1",
    [4] = "Potion2",
    [5] = "Muscle2",
    [6] = "Win1",
    [7] = "Gem2",
    [8] = "Win2",
    [9] = "LimitedPet"
}
for _,arg in ipairs(args) do
    game:GetService("ReplicatedStorage").Events.FreeGift:FireServer(arg)
    wait(.1)
    end
    end
end
})

Tab:AddToggle({
    Name = "Auto Spin Wheel",
