local Section = Tab:AddSection({
	Name = "Redeem All Codes"
})

Tab:AddButton({
Name = "Auto Redeem All Codes",
Callback = function()
local autocodes = true
while autocodes do
local args = {
"EASTER",
"RELEASE",
"EAST3R3V3NNT"
}
for _, arg in ipairs(args) do
game:GetService("ReplicatedStorage"):WaitForChild("Modules"):WaitForChild("CodeModule"):WaitForChild("EnterCode"):InvokeServer(arg)
wait(0.5)
end
end
print("button pressed")
end
})

OrionLib:Init()
